{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Neo4j": {
    "Uri": "bolt://neo4j:7687",
    "Username": "neo4j",
    "Password": "mnemosyne",
    "Database": "neo4j",
    "VectorDimensions": 1024
  },
  "EmbeddingService": {
    "BaseUrl": "http://mnemosyne-embed:8000",
    "TimeoutSeconds": 30,
    "MaxRetryAttempts": 3,
    "RetryDelayMilliseconds": 500
  },
  "LanguageModels": {
    "Configurations": {
      "AnthropicClaude": {
        "Name": "Primary LLM",
        "Provider": "Anthropic",
        "Url": "https://api.anthropic.com",
        "MaxTokens": 200000,
        "ModelName": "claude-3-5-haiku-latest",
        "Enabled": true,
        "AdditionalHeaders": {
        },
        "ProviderSpecificSettings": {
        }
      },
      "OpenRouterGemma": {
        "Name": "Auxiliary LLM",
        "Provider": "OpenRouter",
        "Url": "https://openrouter.ai/api/v1",
        "MaxTokens": 131000,
        "ModelName": "google/gemma-3-4b-it",
        "Enabled": true,
        "AdditionalHeaders": {
        },
        "ProviderSpecificSettings": {
        }
      }
    },
    "DefaultAssignments": {
      "Master": "AnthropicClaude",
      "Auxiliary": "OpenRouterGemma"
    }
  },
  "PipelineStorage": {
    "BasePath": "/app/pipelines"
  }
}